package br.com.senaisp.bauru.secao03.aula06;

import javax.swing.JOptionPane;

public class Exercicio01 {

	public static void main(String[] args) {
		JOptionPane.showConfirmDialog(null,
				"Olá, participante, e bem-vindo ao programa de namoro favorito da América!",
				"Bem-vindo, participante!",
				JOptionPane.YES_NO_OPTION,
				JOptionPane.INFORMATION_MESSAGE);
				
		JOptionPane.showInputDialog(null,
                "Is this a question?",
                "Dialog Title",
                JOptionPane.WARNING_MESSAGE, // tipo de mensagem
                null, // Ícone
                null, // Opções de escolha
                "Type something here.");
	}

}
