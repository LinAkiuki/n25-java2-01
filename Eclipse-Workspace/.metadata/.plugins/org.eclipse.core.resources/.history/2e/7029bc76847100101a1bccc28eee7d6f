package br.com.senaisp.bauru.secao03.aula05;

public class ShoppingCart01 {

	public static void main(String[] args) {
		// Declarar as variáveis do exercicío
		String custName = ""; // Nome do Cliente
		String itemDesc = ""; // Item a ser comprado
		// Montar a mensagem
		String message = custName + " quer comprar " + " um(a)" + " " + itemDesc;
		// Imprimir a mensagem
		System.out.println(message);
	}

}
